{
  "name": "react-frontend",
  "version": "1.0.0",
  "description": "A React template",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "format": "npx prettier --write .",
    "test": "jest --env=jsdom",
    "test:ci": "jest --env=jsdom --passWithNoTests",
    "test:watch": "jest --env=jsdom --watchAll",
    "build": "npm run build:prod",
    "build:dev": "webpack --mode=development --progress",
    "build:prod": "webpack --mode=production --node-env=production --progress",
    "start": "npm run serve",
    "serve": "webpack serve --hot --open --live-reload --port=3000 --progress --history-api-fallback",
    "profile": "webpack --profile",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "extract-translations": "i18next --config ./i18next-parser.config.js"
  },
  "author": "Digio",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.18.9",
    "@babel/core": "^7.18.9",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.3.0",
    "@types/jest": "^28.1.6",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-test-renderer": "^18.0.0",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "babel-jest": "^28.1.3",
    "compression-webpack-plugin": "^10.0.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "i18next-parser": "^6.5.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-watch-typeahead": "^2.0.0",
    "json-server": "^0.17.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react-test-renderer": "^18.2.0",
    "thread-loader": "^3.0.4",
    "ts-loader": "^9.3.1",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "workbox-webpack-plugin": "^6.5.3"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/material": "^5.9.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-i18next": "^11.18.1",
    "react-router-dom": "^6.3.0"
  },
  "lint-staged": {
    "./**/*.{ts,tsx}": [
      "eslint ./src --fix",
      "prettier --write ./src",
      "npm run test:ci"
    ]
  }
}
